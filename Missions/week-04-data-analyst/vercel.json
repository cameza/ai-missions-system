{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/sync/transfers",
      "schedule": "30 16 * * *"
    },
    {
      "path": "/api/sync/transfers", 
      "schedule": "30 4 * * *"
    }
  ],
  "env": {
    "DEADLINE_DAY_MODE": "false",
    "MANUAL_SYNC_TOKEN": "your_manual_sync_token_here",
    "ENABLE_DEADLINE_CRON": "false"
  }
}
